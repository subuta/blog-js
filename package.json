{
  "name": "blog-js",
  "version": "1.0.0",
  "description": "example project built-with js-to-builder and matryoshka.js",
  "main": "src/index.js",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "test": "LOG_QUERIES=true POSTGRES_DB=blog-js-test nyc ava --serial",
    "serve": "LOG_QUERIES=true node dist/main.js",
    "watch": "backpack dev",
    "build": "backpack build",
    "knex": "knex",
    "seed": "knex seed:run",
    "db:migrate": "knex-migrate up",
    "db:migrate:drop": "knex-migrate down --to 0",
    "db:migrate:reset": "npm run db:migrate:drop && npm run db:migrate",
    "mat": "mat"
  },
  "quokka": {
    "babel": true,
    "env": {
      "params": {
        "env": "NODE_PATH=./"
      }
    }
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "require": "./test/.setup.js",
    "babel": {
      "extends": ".babelrc",
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "test": "./test"
            }
          }
        ]
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/blog-js.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/blog-js/issues"
  },
  "homepage": "https://github.com/subuta/blog-js#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.34",
    "@babel/preset-env": "^7.0.0-beta.34",
    "ava": "^0.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "backpack-core": "^0.4.3",
    "js-to-builder": "^0.2.2",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.14.2",
    "knex-migrate": "^1.3.1",
    "matryoshka.js": "^0.1.0",
    "nock": "^9.1.4",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "sails-memory": "^0.10.7",
    "supertest": "^3.0.0",
    "sinon": "^4.1.3",
    "pg": "^6.4.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "aws-sdk": "^2.167.0",
    "bluebird": "^3.5.1",
    "boom": "^7.1.1",
    "dotenv": "^4.0.0",
    "history": "^4.7.2",
    "inline-style-prefixer": "^3.0.8",
    "jwks-rsa": "^1.2.1",
    "keycode": "^2.1.9",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-jwt": "^3.3.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-free-style": "^7.0.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-textarea-autosize": "^5.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sails-postgresql": "^0.11.4",
    "uuid": "^3.1.0",
    "waterline": "^0.13.1-9"
  }
}
