{
  "name": "blog-js-front",
  "version": "1.0.0",
  "description": "example project built-with js-to-builder and matryoshka.js",
  "main": "src/index.js",
  "engines": {
    "node": ">=9.0.0"
  },
  "scripts": {
    "build": "poi build",
    "watch": "poi --port 4001",
    "poi": "poi",
    "svg": "svg-sprite -C ./svg-sprite-config.json -D static/assets 'assets/svg/*.svg'",
    "mat": "../node_modules/.bin/mat",
    "test": "NODE_PATH=./ ../node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/blog-js.git"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "require": "babel-register",
    "babel": {
      "extends": ".babelrc",
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "test": "./test"
            }
          }
        ]
      ]
    }
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/blog-js/issues"
  },
  "homepage": "https://github.com/subuta/blog-js#readme",
  "devDependencies": {
    "svg-sprite": "^1.3.7"
  },
  "dependencies": {
    "auth0-js": "^8.12.0",
    "axios": "^0.17.1",
    "color": "^2.0.1",
    "file-saver": "^1.3.3",
    "poi": "^9.6.0",
    "poi-preset-react": "^9.2.1",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-router-dom": "^4.2.2",
    "webfontloader": "^1.6.28"
  }
}
